{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "turbo run start:dev --filter=api",
    "dev:web": "turbo run dev --filter=web",
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:web\""
  },
  "name": "barber",
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "turbo": "^2.5.4"
  }
}
